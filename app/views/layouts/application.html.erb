    <script>window.jQuery || document.write('<script src="http://getbootstrap.com/docs/4.0/assets/js/vendor/jquery-slim.min.js"><\/script>')</script>
    <script src="http://getbootstrap.com/docs/4.0/assets/js/vendor/popper.min.js"></script>
    <script src="http://getbootstrap.com/docs/4.0/dist/js/bootstrap.min.js"></script>
    <script src="http://getbootstrap.com/docs/4.0/assets/js/vendor/holder.min.js"></script>
  </body>
</html>
